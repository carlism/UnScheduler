= form_for @presentation do |f|
  = f.hidden_field :time_slot_id
  = f.hidden_field :room_id
  %p
    = f.label :title
    %br/
    = f.text_field :title
  %p
    = f.label :presenter
    %br/
    = f.text_field :presenter
  %p
    = f.label :contact
    %br/
    = f.text_field :contact
  %p
    = f.submit
    =link_to "Cancel", event_event_date_url(@event_date.event, @event_date)